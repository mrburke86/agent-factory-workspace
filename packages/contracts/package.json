{
  "name": "@acme/contracts",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    }
  },
  "scripts": {
    "snapshot:current": "node --no-warnings dist/scripts/generate-snapshot.js",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "check:breaking": "node --no-warnings dist/scripts/generate-snapshot.js && node --no-warnings dist/src/compat/breaking-check.js"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "typescript": "^5.5.4"
  }
}
