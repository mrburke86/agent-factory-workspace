{
  "name": "@acme/evals",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "eval:smoke": "node --no-warnings dist/scripts/eval_smoke.js",
    "check:no-agent-runtime-copies": "node --no-warnings dist/scripts/check_no_agent_runtime_copies.js",
    "check:agent-manifests": "node --no-warnings dist/scripts/check_agent_manifests.js",
    "eval:l2-config-validate": "node --no-warnings dist/scripts/eval_l2_config_validate.js",
    "eval:context-gather": "node --no-warnings dist/scripts/eval_context_gather.js",
    "eval:agent-retrieval-smoke": "node --no-warnings dist/scripts/agent_retrieval_smoke.js",
    "eval:agent-runner-smoke": "node --no-warnings dist/scripts/agent_runner_smoke.js",
    "eval:full": "node --no-warnings dist/scripts/eval_full.js"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "typescript": "^5.5.4"
  }
}
